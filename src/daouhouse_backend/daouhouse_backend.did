type Profile = record {
  username : text;
  user_id : principal;
  email_id : text;
  profile_img : blob;
};
type Profileinput = record {
  username : text;
  email_id : text;
  profile_img : blob;
};
type Result = variant { Ok; Err : text };
service : () -> {
  create_profile : (Profileinput) -> (Result);
  delete_profile : () -> ();
  get_profile : () -> (opt Profile) query;
  greet : (text) -> (text) query;
  update_profile : (Profileinput) -> (Result);
}
